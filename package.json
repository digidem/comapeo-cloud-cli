  "keywords": [
    "cli",
    "comapeo",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "chalk": "^5.3.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.10",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "pretty-quick": "^4.0.0",
    "simple-git-hooks": "^2.11.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18"
  }
