{
  "name": "comapeo-cloud-cli",
  "version": "1.0.0",
  "description": "CLI tool for interacting with Comapeo Cloud API",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "comapeo": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "prepare": "npm run build",
    "start": "node dist/index.js",
    "lint": "prettier --write ."
  },
  "simple-git-hooks": {
    "pre-commit": "bun pretty-quick --staged"
  },
  "files": [
